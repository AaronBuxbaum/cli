{
  "name": "@percy/cli-command",
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types/index.d.ts"
  ],
  "scripts": {
    "build": "babel --root-mode upward src --out-dir dist",
    "lint": "eslint --ignore-path ../../.gitignore .",
    "test": "cross-env NODE_ENV=test mocha",
