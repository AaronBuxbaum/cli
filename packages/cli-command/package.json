  "mocha": {
    "require": "../../scripts/babel-register"
  },
  "oclif": {
    "bin": "percy",
    "hooks": {
      "init": "./dist/hooks/init"
    }
  },
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/plugin-help": "^2.2.3",
    "@percy/config": "1.0.0",
    "@percy/logger": "1.0.0"
  }
}
