{
  "name": "@percy/cli",
  "version": "1.0.0",
  "license": "MIT",
  "main": "index.js",
  "bin": {
    "percy": "./bin/run"
  },
  "files": [
    "./bin",
    "./index.js",
    "./oclif.manifest.json"
  ],
  "scripts": {
    "lint": "eslint --ignore-path ../../.gitignore .",
    "postbuild": "oclif-dev manifest"
  },
  "oclif": {
    "bin": "percy",
    "plugins": [
      "@percy/cli-config",
      "@percy/cli-exec",
      "@percy/cli-build",
      "@percy/cli-snapshot",
      "@percy/cli-upload",
      "@oclif/plugin-help"
    ]
  },
  "dependencies": {
    "@oclif/plugin-help": "^3.1.0",
    "@percy/cli-config": "1.0.0",
    "@percy/cli-exec": "1.0.0",
    "@percy/cli-build": "1.0.0",
    "@percy/cli-snapshot": "1.0.0",
    "@percy/cli-upload": "1.0.0"
  }
}
